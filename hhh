import java.io.FileInputStream;
import java.io.FileOutputStream;
import java.nio.charset.Charset;
import org.apache.commons.io.IOUtils;
public class TMPFileReader {
public static void main(String[] args) throws Exception {
String inputFileName = "C://Users//chenna_gottam//Desktop//Test.txt";
FileInputStream fileInputStream = new FileInputStream(inputFileName);
String content = IOUtils.toString(fileInputStream, Charset.defaultCharset());
content = content.replaceAll("-", "9");
String outputFileName = "C://Users//chenna_gottam//Desktop//TestOut.txt";
FileOutputStream fileOutputStream = new FileOutputStream(outputFileName);
IOUtils.write(content, new FileOutputStream(outputFileName), Charset.defaultCharset());
fileInputStream.close();
fileOutputStream.close();
}
}
